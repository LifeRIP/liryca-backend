    "paths": {
        "/api/v1/auth/logout": {
            "get": {
                "tags": [
                    "auth"
                ],
                "summary": "Logout the authenticated user",
                "description": "Logout the authenticated user",
                "operationId": "logout",
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Logged out successfully"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Error: Unauthorized",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Unauthenticated"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "sanctum": []
                    }
                ]
            }
        },
        "/api/v1/song/top-by-user/{userId}": {
            "get": {
                "tags": [
                    "song"
                ],
                "summary": "Get top songs by user",
                "description": "Returns the top 10 songs by a user based on playback count",
                "operationId": "top-by-user",
                "parameters": [
                    {
                        "name": "userId",
                        "in": "path",
                        "description": "ID of the user",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "data": {
                                            "type": "array",
                                            "items": {
                                                "properties": {
                                                    "song_id": {
                                                        "type": "integer",
                                                        "example": 1
                                                    },
                                                    "album_id": {
                                                        "type": "integer",
                                                        "example": 1
                                                    },
                                                    "time": {
                                                        "type": "string",
                                                        "example": "00:01:38"
                                                    },
                                                    "genre": {
                                                        "type": "string",
                                                        "example": "Reggaeton"
                                                    },
                                                    "song_url": {
                                                        "type": "string",
                                                        "example": "https://i.scdn.co/image/ab67616d0000b273b62a2ec2d61d48f34a368144"
                                                    },
                                                    "is_active": {
                                                        "type": "boolean",
                                                        "example": true
                                                    },
                                                    "created_at": {
                                                        "type": "string",
                                                        "format": "date-time",
                                                        "example": "2024-11-18T00:20:29.000000Z"
                                                    },
                                                    "updated_at": {
                                                        "type": "string",
                                                        "format": "date-time",
                                                        "example": "2024-11-18T00:20:29.000000Z"
                                                    },
                                                    "song_name": {
                                                        "type": "string",
                                                        "example": "Mírame"
                                                    },
                                                    "artist_id": {
                                                        "type": "integer",
                                                        "example": "264e1f6c-52ec-48ea-bfb1-13100f8b5cf3"
                                                    },
                                                    "album_image": {
                                                        "type": "string",
                                                        "example": "https://i.scdn.co/image/ab67616d0000b273b62a2ec2d61d48f34a368144"
                                                    },
                                                    "artist_name": {
                                                        "type": "string",
                                                        "example": "Blessd"
                                                    },
                                                    "is_liked": {
                                                        "type": "boolean",
                                                        "example": true
                                                    }
                                                },
                                                "type": "object"
                                            }
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Error: Not Found",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "error": {
                                            "type": "string",
                                            "example": "No songs found for this user"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "sanctum": []
                    }
                ]
            }
        },
